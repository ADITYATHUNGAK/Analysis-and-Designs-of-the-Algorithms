#include <stdio.h>
#include <stdlib.h>
#include <time.h>

void main()
{
    int i, j, n, temp;
    printf("Enter the size of the array\n");
    scanf("%d", &n);
    int a[n];
    srand(time(NULL));
    printf("Enter the array elements\n");
    for(i = 0; i < n; i++)
    {
        a[i] = rand() % 25000;
    }

    for(i = 0; i < n-1; i++)
    {
        for(j = i+1; j < n; j++)
        {
            if(a[i] > a[j])
            {
                temp = a[i];
                a[i] = a[j];
                a[j] = temp;
            }
        }
    }

    printf("The elements after sorting in the array\n");
    for(i = 0; i < n; i++)
    {
        printf("%d\t", a[i]);
    }

    clock_t end, start;
    double timetaken = (double)(end - start) / CLOCKS_PER_SEC;
    printf("\nTime taken is the %lf\n", timetaken);
}
